view.bill-list {
  border-top: 1px solid #F2F3F4;
}
view.bill-list .item {
  margin-bottom: 20rpx;
  background: #fff;
}
view.bill-list .item .time {
  font-size: 12px;
  padding: 10rpx 40rpx 10rpx 0;
  margin-left: 40rpx;
  border-bottom: 1px solid #F2F3F4;
}

view.bill-list .item span.bill-time {
  float: left;
}
view.bill-list .item span.bill-stat {
  float: right;
}
view.bill-box {
  padding: 30rpx 40rpx;
}
view.bill-box .pic {
  float: left;
  width: 160rpx;
  height: 160rpx;
  margin-right: 30rpx;
}
view.bill-box .pic image {
  width: 160rpx;
  height: 160rpx;
}
view.bill-box .info {
  height: 160rpx;
}
view.bill-box .info .name {
  font-size: 16px;
  font-weight: bold;
  margin-bottom: 15rpx;
}
view.bill-box .det {
  position: absolute;
  bottom: 0;
  line-height: 40rpx;
  font-size: 12px;
  color: #9498A5;
  left: 190rpx;
}
view.bill-box .det em, view.bill-box .det cite {
  display: inline-block;
  margin-right: 10rpx;
}
view.bill-box .btn-box {
  position: absolute;
  width: 242rpx;
  right: 0;
  bottom: 5rpx;
}

view.has-null {
  background: url('data:image/png;base64,') no-repeat center 80rpx;
  background-size: 140rpx auto; 
  padding: 260rpx 40rpx 0;
  text-align: center;
  color: #c3c4c7;
}
view.has-null view.btn-box2 {
  padding: 200rpx 180rpx;  
}