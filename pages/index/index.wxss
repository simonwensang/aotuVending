/**index.wxss**/
.has-null-wine {
  padding-top: 120rpx;
  background: url('data:image/png;base64,') no-repeat center 80rpx;
  background-size: 140rpx auto; 
  padding: 260rpx 40rpx 0;
  text-align: center;
  color: #c3c4c7;
}

.container{
  padding:30rpx;
}
.item-wine {
  display: block;
  width: 100%;
  background: #fff;
  padding: 40rpx;
  border-radius: 10rpx;
  box-shadow: 0 4px 10px rgba(0,0,0,.03);
  margin-bottom: 30rpx;
}
view.thumb {
  float: left;
  width: 220rpx;
  height: 220rpx;
  margin-right: 30rpx;
}
view.thumb image {
  width: 220rpx;
  height: 220rpx;
}
view.info {
  float: left;
  height: 220rpx;
  width: 360rpx;
  position: relative;
}
view.wine-name {
  font-size: 18px;
  font-weight: bold;
  margin-bottom: 10rpx;
}
view.price {
  color: #F3775D;
  font-size: 18px;
}
view.price span {
  font-size: 12px;
  margin-left: 5px;
}
view.remain {
  margin-top: 4rpx;
  font-size: 12px;
  color: #939497;
}
view.go-buy {
  position: absolute;
  right: 40rpx;
  bottom: 30rpx;
  background: #f2f3f4;
  padding: 6rpx;
  border-radius: 50rpx;
}
view.go-buy .btn-index-buy {
  width: 160rpx;
  height: 50rpx;
  background: #f3775d;
  color: #fff;
  line-height: 50rpx;
  text-align: center;
  border-radius: 25rpx;
  font-size: 14px;
  box-shadow: 0 4rpx 10rpx rgba(0,0,0,.1);
}
view.go-buy .has-w-n {
  width: 160rpx;
  height: 50rpx;
  color: #939497;
  line-height: 50rpx;
  text-align: center;
  border-radius: 25rpx;
  font-size: 14px;
}
view.dia-modal {
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
  top: 0;
  background: rgba(41,49,76,.45);
}
view.modal-box {
  background: #fff;
  padding: 60rpx 40rpx;
  width: 640rpx;
  position: absolute;
  top: 50%;
  left: 55rpx;
  border-radius: 20rpx;
  height: 820rpx;
  margin-top: -410rpx;
}
view.pic {
  text-align: center;
}
view.pic image {
  width: 220rpx;
  height: 220rpx;
  border: 2px solid #EDEEF0;
  border-radius: 110rpx;
  margin-bottom: 10rpx;
}
view.modal-box view.name {
  text-align: center;
  font-size: 16px;
  font-weight: bold;
}
view.modal-box .slider {
  padding-top: 30rpx;
  text-align: center;
}
view.modal-box .slider-title {
  font-size: 12px;
  color: #939497;
  margin-bottom: 20rpx;
}
.fast-qua {
  padding: 15rpx 0;
}
.fast-qua view {
  display: inline-block;
  border: 1rpx solid #d5d7d9;
  line-height: 1;
  padding: 15rpx 35rpx;
  margin: 0 8rpx;
  border-radius: 30rpx;
}
.fast-qua view.hover {
  border-color: #d5d7d9;
  background: #f5f7f9;
}
.fast-qua view.active {
  background: #f3775d;
  color: #fff;
  border-color: #f3775d;
}
view.modal-box .slider-box {
  padding: 40rpx 0 60rpx;
}
view.btn-box {
  margin-bottom: 40rpx;
  padding: 0 50rpx;
}
view.btn .cost {
  float: left;
  margin-left: 50rpx;
}
view.btn .cost span {
  font-size: 12px;
}
view.btn .text {
  float: right;
  margin-right: 50rpx;
}
view.modal-box view.close {
  padding: 8rpx;
  position: absolute;
  right: 12rpx;
  top: 12rpx;
  z-index: 6666;
}
view.modal-box view.close span {
  position: relative;
  width: 44rpx;
  height: 44rpx;
  display: block;
}
view.modal-box view.close span:before, view.modal-box view.close span:after {
  background: #d3d4d7;
  position: absolute;
  content: ' ';
  display: block;
}
view.modal-box view.close span:before {
  width: 44rpx;
  height: 4rpx;
  transform: rotate(45deg);
  top: 20rpx;
}
view.modal-box view.close span:after {
  width: 4rpx;
  height: 44rpx;
  transform: rotate(-135deg);
  top: 0rpx;
  left: 20rpx;
}
view.modal-box view.close:hover span:before, view.modal-box view.close:hover span:after {
  background: #F3775D;
}
